{
  "name": "learn-x-frontend",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://var-globe-charlie-scheduled.trycloudflare.com",
    "NEXT_PUBLIC_SOCKET_URL": "https://var-globe-charlie-scheduled.trycloudflare.com"
  },
  "functions": {
    "frontend/src/app/**/*.tsx": {
      "maxDuration": 30
    }
  }
}
